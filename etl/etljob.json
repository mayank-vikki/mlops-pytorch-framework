{
    "Name": "",
    "Description": "Python ETL Shell Job to featurize dataset for PyTorch model",
    "Role": "",
    "ExecutionProperty": {
        "MaxConcurrentRuns": 1
    },
    "Command": {
        "Name": "pythonshell",
        "ScriptLocation": "",
        "PythonVersion": "3"
    },
    "DefaultArguments": {
        "--job-language": "python",
        "--additional-python-modules": "torch==2.0.0,numpy==1.21.0,pandas==1.4.0,scikit-learn==1.0.2",
        "--enable-metrics": "true"
    },
    "Timeout": 15,
    "MaxCapacity": 1.0 
}

